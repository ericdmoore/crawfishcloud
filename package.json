{
  "name": "crawfishcloud",
  "version": "0.1.0",
  "description": "A Streaming S3 Bucket Glob Crawler",
  "main": "build/index.min.js",
  "types": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "npm run build:esbuild",
    "build:esbuild": "esbuild src/index.ts --format=iife --platform=node --minify --bundle > build/index.min.js",
    "build:check": "esbuild src/index.ts > /dev/null",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdmoore/crawfishcloud.git"
  },
  "keywords": [
    "S3",
    "Bucket",
    "Stream",
    "vfile",
    "vinyl",
    "glob"
  ],
  "license": "CDDL-1.0",
  "author": {
    "name": "Eric D Moore",
    "url": "https://im.ericdmoore.com",
    "email": "eric.d.moore+crawfishcloud@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/ericdmoore/crawfishcloud/issues"
  },
  "homepage": "https://github.com/ericdmoore/crawfishcloud#readme",
  "peerDependencies": {
    "aws-sdk": "^2.870.0",
    "vfile": "^4.2.1",
    "vinyl": "^2.2.1"
  },
  "dependencies": {
    "micromatch": "^4.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^26.0.22",
    "@types/lodash.isequal": "^4.5.5",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^14.14.35",
    "@types/vinyl": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "chalk": "^2.4.2",
    "esbuild": "^0.10.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lodash.isequal": "^4.5.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3"
  }
}
